{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "ShabasherDbContext": "${DB_CONNECTION_STRING}"
  },
  "JwtOptions": {
    "SecretKey": "${JWT_SECRET}",
    "ExpiresHours": "${JWT_HOURS}"
  }
}